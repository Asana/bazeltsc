{
  "name": "bazeltsc",
  "version": "1.0.0",
  "description": "A Bazel persistent worker for compiling TypeScript",
  "keywords": [
    "compiler",
    "tsc",
    "javasript"
  ],
  "engines": {
    "npm": ">=4.0"
  },
  "bin": {
    "bazeltsc": "bin/bazeltsc"
  },
  "files": [
    "bin/*",
    "dist/*",
    "tsc.bzl"
  ],
  "repository": "Asana/bazel-tsc",
  "scripts": {
    "build": "./build.sh",
    "prepare": "npm run build",
    "clean": "rm -rf node_modules lib dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Mike Morearty (https://github.com/mmorearty)",
  "license": "MIT",
  "peerDependencies": {
    "typescript": ">= 2.0.2"
  },
  "devDependencies": {
    "@types/google-protobuf": "^3.2.7",
    "@types/node": "^7.0.12",
    "google-protobuf": "^3.2.7",
    "ts-protoc-gen": "^0.4.0",
    "tslint": "^5.8.0",
    "typescript": "^2.5.3",
    "webpack": "^3.6.0"
  }
}
